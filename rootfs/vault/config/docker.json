{
    "storage": {
        "raft": {
            "path": "/vault/file"
        }
    },
    "listener": [
        {
            "tcp": {
                "address": "0.0.0.0:8200",
                "tls_disable": true
            }
        }
    ],
    "telemetry": {
        "prometheus_retention_time": "24h",
        "disable_hostname": true
    },
    "pid_file": "/vault/config/vault.pid",
    "ui": true
}
